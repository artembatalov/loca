{
  "name": "loca",
  "displayName": "LOCA",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:loca.recalculateMetrics"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "loca.recalculateMetrics",
        "title": "Recalculate LOCA Metrics"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3"
  }
}
